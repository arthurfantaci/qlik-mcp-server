# Test Environment Configuration
# Copy this file to .env.test and configure with your test Qlik Sense server details

# Qlik Sense Test Server Configuration
QLIK_SERVER_URL=test-qlik-server.example.com
QLIK_SERVER_PORT=4747
QLIK_USER_DIRECTORY=INTERNAL
QLIK_USER_ID=sa_engine

# Test Application IDs
# Replace these with your actual test application IDs when available
TEST_APP_ID=12345678-abcd-1234-efgh-123456789abc
TEST_APP_NAME=Test Dashboard
TEST_SHEET_ID=sheet-123-abc
TEST_CONTAINER_ID=container1

# Test Configuration
SKIP_INTEGRATION_TESTS=true  # Set to false when Qlik server is available
TEST_TIMEOUT=30  # Seconds
WEBSOCKET_TIMEOUT=10000  # Milliseconds

# Mock Data Configuration
USE_MOCK_DATA=true  # Use mock data when Qlik server is unavailable
MOCK_DATA_PATH=tests/mock_data

# Debug Settings
DEBUG_MODE=false
VERBOSE_OUTPUT=false